{
  "version": 2,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 14,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "development",
      "inherits": "development-linux"
    },
    {
      "name": "release",
      "inherits": "release-linux"
    }
  ],
  "buildPresets": [
    {
      "name": "development",
      "configurePreset": "development",
      "configuration": "Debug"
    },
    {
      "name": "release",
      "configurePreset": "release",
      "configuration": "Release"
    }
  ]
}
